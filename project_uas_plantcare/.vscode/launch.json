{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Flutter Web (no CORS)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "debug",
        "deviceId": "chrome",
        "args": [
          "--web-browser-flag=--disable-web-security",
          "--web-browser-flag=--user-data-dir=/tmp/chrome_dev"
        ]
      }
    ]
  }
  